{
    "name": "dqneo/skeleton",
    "description": "a skeleton package which does nothing at all",
    "type": "library",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.7"
    },
    "autoload": {
        "psr-4" : {
            "DQNEO\\Skeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "DQNEO\\Skeleton\\": "tests/"
        }
    },
    "bin" : [
        "skeleton"
    ],
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix"
    },
    "authors": [
        {
            "name": "DQNEO",
            "email": "dqneoo@gmail.com"
        }
    ],
    "license": "MIT"
}
